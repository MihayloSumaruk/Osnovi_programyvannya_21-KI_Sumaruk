def convert_input_to_list(input_string):
    return [int(x) if x.isdigit() else x for x in input_string.split()]
def sortyvannya(lst):
    unique_list = list(set(lst))
    numbers = sorted([x for x in unique_list if type(x) in (int, float)])
    strings = sorted([x for x in unique_list if type(x) == str])
    return numbers + strings
input_string = input("Введіть список чисел через пробіл: ")
nesort_spisok = convert_input_to_list(input_string)
sort_spisok = sortyvannya(nesort_spisok)
print("Невідсортований список:", nesort_spisok)
print("Відсортований список:", sort_spisok)
